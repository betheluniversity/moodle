html,body {
	margin: 0;
	padding: 0;
	background: #fff;
	color: #333;
}

html,body,h1,h2,h3,h4,h5,h6,p,ul,ol,dl,textarea {
	font-family: "Lucida Sans Unicode",sans-serif;
}
/* background: #f1eee7 url([[pix:theme|bkg]]) no-repeat 100% 0; */

ul, ol {
	margin: 0 1.5em;
}

/*link colors */
a:link,
a:visited, .tabtree .tabrow0 li a  {
    color: #005ba3;
    text-decoration: none;
}

a:hover,
a:active {
    color: #002484;
    text-decoration: none;
}

a:active {
    outline: 0;

/* for browsers that understand */
}

a:focus {
    outline: 0;

/* for browsers that understand */
}

/* real main headings */
h2.headingblock, .sitetopic h2 {
  border-width:0 0 0px 0;
  padding-bottom:3px;
  font: 21px/24px "superclarendon-1","superclarendon-2",Georgia,"Times New Roman",Times,serif;
  font-weight: 500;
  border-bottom: 1px solid #f5af34;
}

/*outercontainer */
#wrapper {
	
	clear: both;
}

/* change the default content size */
#page {
 font-size: 100%;
}

/*outer container around moodle content, set padding here for moodle content if necessary */
#page-content-wrapper {
	background: url([[pix:theme|lcol]]) repeat-y top left;
	background-color: transparent;
}

.side-post-only #page-content-wrapper, .content-only #page-content-wrapper {
	background: none;
}

#page-content-wrapper2 {
	background: url([[pix:theme|rcol]]) repeat-y 100% 0;
	background-color: transparent;
	clear: both;
}

.side-pre-only #page-content-wrapper2, .content-only #page-content-wrapper2 {
	background: none;
}

#page-content {
	float: none;
}

.clearer {
	clear: both;
}


/* header, get rid of the float from base theme */
#headwrap {
	height: 65px;
	background-color: #005ba3;
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#005ba3', endColorstr='#00427a'); /* for IE */
	background: -webkit-gradient(linear, left top, left bottom, from(#005ba3), to(#00427a)); /* for webkit browsers */
	background: -moz-linear-gradient(top,  #005ba3,  #00427a); /* for firefox 3.6+ */
	clear: both;
}

#page-header {
	float: none;
	
  width:100%;
  min-width: 960px;
  
  margin-left: auto;
  margin-right: auto;
  clear: both;
  position: relative;
}

/*logo if any*/
#logo {
	float: left;
	margin: 15px 25px 15px 15px;
	display: block;
}

h2.main {
	margin-top: 15px;
}

#dropdown{
	float: left;
	margin: 18px 0;
	padding: 0;
}

#dropdown li {
	float: left;
	display: block;
	list-style: none;
	margin: 0px 3px 0;
}

#dropdown li a { 
	font: bold 11px/15px Arial, Helvetica, sans-serif;
	color: #ffffff;
	margin: 0;
	display: block;
	padding: 5px 10px;
	background: transparent url(../bethel/images/bkg-nav.png) bottom left repeat-x;
	filter:  progid:DXImageTransform.Microsoft.gradient(startColorstr='#3180bd', endColorstr='#005ba3'); /* for IE */
	background: -moz-linear-gradient(
		top,
		#3180bd 0%,
		#005aa3);
	background: -webkit-gradient(
		linear, left top, left bottom, 
		from(#3180bd),
		to(#005aa3));
	border-radius: 4px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border: 1px solid #081a2f;
	-moz-box-shadow:
		0px 1px 1px rgba(255,255,255,0.1),
		inset 0px 0px 2px rgba(255,255,255,0.15);
	-webkit-box-shadow:
		0px 1px 1px rgba(255,255,255,0.1),
		inset 0px 0px 2px rgba(255,255,255,0.4);
	text-shadow:
		0px 1px 1px rgba(000,000,000,0.6),
		0px 1px 0px rgba(255,255,255,0);
}

#dropdown li a:hover {
	background-image: none;
	background: #004377;
	-moz-box-shadow:
		0px 1px 1px rgba(255,255,255,0.1),
		inset 0px 0px 2px rgba(0,0,0,.5);
	-webkit-box-shadow:
		0px 1px 1px rgba(255,255,255,0.1),
		inset 0px 0px 2px rgba(0,0,0,.5);
}


.headermenu {
  position: absolute;
  color: #fff;
  top: 10px;
  right: 22px;
  font-size: 11px;
  font-weight: bold;
  font-family: "Lucida Grande","Lucida Sans Unicode",sans-serif;
}

.headermenu a {
	color: #f8b231;
}

.headermenu .logininfo, .headermenu #chooselang, .headermenu #chooselang div, .headermenu #chooselang select {
	display: inline;
}

.headermenu #chooselang, .headermenu .langmenu {
	display: none;
}

/* Navbar */
.course-title {
	text-align: center;
	
	color: #333;
	text-shadow: 1px 1px 1px #eee;
	background: #ccc;
	font-size: 1.2em;
	font-weight: bold;
	line-height: 2.6em;
	background: #EEEEEE; /* old browsers */
background: -moz-linear-gradient(top, #EEEEEE 0%, #BBBBBB 100%); /* firefox */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#EEEEEE), color-stop(100%,#BBBBBB)); /* webkit */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#EEEEEE', endColorstr='#BBBBBB',GradientType=0 ); /* ie */
border-bottom: 1px solid #777;
}

.navbar {
  border: none;
  border-bottom: 1px solid #bbb;
  font-size: 12px;
  margin: 0;
  padding: 3px 10px 2px;
  background: #e2edf6;
  clear: both;
}

.breadcrumb {
	font-size: 10px;
	font-weight: 600;
	padding-top: 2px;
	padding-bottom: 2px;
}

.navbar .breadcrumb li, .navbar .breadcrumb li a  {
 font-family: "Lucida Grande","Lucida Sans Unicode",sans-serif;
 line-height: 2em;
}

.navbar .sep {
   color: #999;
}

/* footer */
#page-footer {
	float: none;
	border-top: 1px solid #bbb;
	margin-top: 0px;
	text-align: left;
}

#footerinner {
	padding: 10px;
}

#page-footer .logininfo {
    float: right;
    margin-top: -34px;
}
/* forms and boxes */
.mform fieldset {
    border-color: #e5e5e5;
}

.mform legend.ftoggler {
	color: #333;
    border: 1px solid #e5e5e5;
    background: #fff url([[pix:theme|gradient]]) repeat-x bottom left;
    padding: 3px 10px;
}

.generalbox {
    border-color: #e5e5e5;
    padding: 5px;
}

.sitetopic, .categorybox {
	border: none;
	padding: 0px;
}

#intro.generalbox {
    width: 75%;
    background: #fafafa;
    border: 1px solid #dadada;
    padding: 10px;
    margin: 1em auto;
}

#notice,
.redirectmessage {
    text-align: center;
    border-color: #e5e5e5;
    padding: 5px;
}

/* success or error message */
div.notifysuccess {
    width: 80%;
    margin: 0 auto;
    font-size: 15px;
    color: #008500;
    border: 3px solid #dadada;
    text-align: center;
    background: #fff url([[pix:theme|check]]) no-repeat 2% 50%;
    padding: 5px;
}

div.box.errorbox {
    border: 1px solid #dadada;
    color: #ff0000;
    background: #fff url([[pix:theme|error]]) no-repeat 1% 3%;
    padding: 10px;
}

div.redirectmessage {
    width: 60%;
    margin: 10px auto 0 auto;
    border: none;
    font-size: 13px;
    background: #fff url([[pix:theme|ajax-loader]]) no-repeat 50% 100%;
    padding: 20px;
}

.tabtree .tabrow0 li a {
   background: #fff url([[pix:theme|gradient]]) repeat-x top left;
   }

/* Tables */

#page-mod-glossary-editcategories .generalbox,
#page-user-action_redir .generaltable,
#page-user-index #participants,
.path-mod-forum .forumheaderlist {
    border-width: 1px;
    border-color: #eee;
    border-style: solid;
}

/*table header */
#page-mod-glossary-editcategories .generalbox th,
#page-user-action_redir .generaltable th,
#page-user-index #participants th,
.path-mod-choice .results th,
.path-mod-forum .forumheaderlist th, #page-admin-course-category .generalbox th, .editcourse .header, .results .header, #attempts .header, .generaltable .header, .plugincompattable th, .environmenttable th, .forumheaderlist th {
    color: #333;
    background: #fff url([[pix:theme|gradient]]) repeat-x bottom left;
    border-bottom: 1px solid #dadada;
    }

#page-user-action_redir .generaltable td,
#page-user-index #participants td,
.path-mod-forum .forumheaderlist td {
    border-color: #dadada;
    border-width: 0 1px 1px 0;
    border-style: solid;
    border-collapse: collapse;
    padding: 2px 8px;
}

#page-user-action_redir .generaltable .r1 td,
#page-user-index #participants .r0 td,
.path-mod-choice .results .count,
.path-mod-forum .forumheaderlist .r1 td, .generaltable .r0 td {
    background: #fafafa;
}


a.dimmed, a.dimmed_text, .dimmed_text, .dimmed_text a, .dimmed_category, .dimmed_category a {
    color: #AAAAAA;
}

/** yui overlays **/

div.yui3-widget.yui3-overlay.yui3-widget-positioned {
    border: 1px solid #cccccc;
}

div.yui3-overlay-content.yui3-widget-stdmod {
    border: 1px solid #fff;
    background: #ffff66 url([[pix:theme|alert-overlay]]) repeat-x;
}

div.yui3-widget-bd {
    border: none;
    background: none;
}

div.yui3-widget-bd h1.helpheading {
    font-size: 16px !important;
    font-weight: 800;
    margin-top: 5px;
}

.yui-module.yui-overlay.yui-panel div.hd {
    background: #eeeeee;
    }

/* report fixes and grade table fixes */
/* report fixes and grade table fixes */
.path-course-report #region-main-box #region-main .region-content, .path-course-report #page-content #region-main-box #region-post-box #region-main-wrap #region-main .region-content, .pagelayout-report .no-overflow {
	overflow: auto;
}

.pagelayout-report #page-content {
    overflow: hidden;
} 

.overview-tablecontainer, .graph {
	overflow: auto;
}

table#completion-progress {
	display: block !important;
    overflow: auto;
    width: 100%;
}

#page-admin-plugins #plugins-control-panel {
    display: block;
    overflow: auto;
}

/*My Moodle Page */
.path-my .coursebox .info {
    float: none;
    text-align: left;
    width: 90%;
}

#page-content #region-post-box {
    padding-top: 0;
}

#page-content #region-main {
    padding: 15px 20px;
}

.side-post-only #page-content #region-main-box #region-post-box #region-main-wrap #region-main {
    padding-left: 15px;
}

.side-pre-only #page-content #region-main-box #region-post-box #region-main-wrap #region-main {
    padding-right: 15px;
}

/* Moodle 2.x.x Code
-------------------------*/
table#user-grades .catlevel2 {
background-color:#f9f9f9;
}

table#user-grades tr.avg td.cell {
background-color:#efefff;
font-weight:700;
color:#00008B;
}

table#user-grades tr.odd td.cell {
background-color:#efefef;
white-space:nowrap;
}

table#user-grades tr td.overridden {background-color:#F3E4C0;}
table#user-grades tr.odd td.overridden {background-color:#EFD9A4;}

table#user-grades tr td.ajaxoverridden {background-color:#FFE3A0;}
table#user-grades tr.odd td.ajaxoverridden {background-color:#FFDA83;}

table#user-grades tr.even td.excluded {
background-color:#EABFFF;
}

table#user-grades tr.odd td.excluded {
background-color:#E5AFFF;
}

table#user-grades tr.odd th.header {
background-color:#efefef;
background-image:none;
}

table#user-grades tr.groupavg td.cell {
background-color:#efffef;
font-weight:700;
color:#006400;
}

/* Bethel Custom Changes */
.summary .no-overflow {
overflow: inherit;
}

.label .no-overflow {
overflow:inherit;
}

